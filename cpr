      "cell_type": "code",
      "source": [
        "def prob_a_or_b(a,b,all_possible_outcomes):\n",
        "  proba = len(a)/len(all_possible_outcomes)\n",
        "  prob_b = len(b)/len(all_possible_outcomes)\n",
        "  inter = a.intersection(b)\n",
        "  prob_inter = len(inter)/len(all_possible_outcomes)\n",
        "  return(proba+prob_b - prob_inter)\n",
        "evens = {2,4,6}\n",
        "gt2 = {3,4,5,6}\n",
        "apr = (1,2,3,4,5,6)\n",
        "print(prob_a_or_b(evens,gt2,apr))"
      ],
