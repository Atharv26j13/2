          "name": "stdout",
          "text": [
            "Average iterations taken: [52, 52, 52, 53, 53, 50, 53, 50, 53, 52]\n"
          ]
        }
      ],
      "source": [
        "import random\n",
        "import matplotlib.pyplot as plt\n",
        "logavg = []\n",
        "rangemin = -1\n",
        "rangemax = 1\n",
        "rangeitem = random.uniform(-1.0,1.0)\n",
        "rangeguess = 0\n",
        "a = 0\n",
        "while a != 10:\n",
        "  rangemin = -1\n",
        "  rangemax = 1\n",
        "  rangeitem = random.uniform(-1.0,1.0)\n",
        "  rangeguess = 0\n",
        "  log = []\n",
        "  ranges = []\n",
        "  while rangeguess != rangeitem:\n",
        "    rangeguess = (rangemax + rangemin)/2\n",
        "    if rangeguess > rangeitem:\n",
        "      rangemax = rangeguess\n",
        "    else:\n",
        "      rangemin = rangeguess\n",
        "    log.append(rangeguess)\n",
        "    range.append(rangemax - rangemin)\n",
        "  print(\"range:\",ranges)\n",
        "  print(\"log\",log)\n",
        "  print(\"iterations\",len(log))\n",
        "  print(\"Output\",rangeguess)\n",
        "  print(\"Required output\",rangeitem)\n",
        "  if rangeguess > 0:\n",
        "    plt.title(\"Positive\")\n",
        "  else:\n",
        "    plt.title(\"Negative\")\n",
        "  plt.xlabel(\"Iteration\")\n",
        "  plt.ylabel(\"Value\")\n",
        "  plt.plot(log)\n",
        "  logavg.append(len(log))\n",
        "  plt.plot(ranges)\n",
        "  plt.show()\n",
        "  a +=1\n",
        "print(\"Average iterations taken:\",logavg)\n"
      ]
    },
