        "import random\n",
        "a = int(input(\"Enter your guess 1-100: \"))\n",
        "l = []\n",
        "for i in range(100):\n",
        "  l.append(i)\n",
        "x = random.choice(l)\n",
        "if random.choice(l) == a:\n",
        "  print('YOU WIN!')\n",
        "else:\n",
        "  print('YOU LOSE!')\n",
        "  print('The number was: ', x)"
